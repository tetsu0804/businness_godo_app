<h1>社員一覧</h1>
<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">氏名</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <th scope="row"><%= user.id %></th>
        <td><%= link_to "#{user.last_name} #{user.first_name}", user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
